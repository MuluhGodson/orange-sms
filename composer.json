{
    "name": "mediumart/orange-sms",
    "type": "library",
    "license": "MIT",
    "keywords": ["orange", "orange-api", "sms", "laravel"],
    "description": "A php library to interact with the orange sms api for MiddleEast and Africa.",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "guzzlehttp/guzzle": "~6.0",
        "mediumart/notifier": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Orange\\SMS\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orange\\SMS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mediumart\\Orange\\SMS\\Laravel\\OrangeSmsServiceProvider"
            ]
        }
    }
}
